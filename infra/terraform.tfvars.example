# terraform.tfvars - 변수 값 설정

aws_region    = "ap-northeast-2"
project_name  = "spring-vote"
environment   = "dev"

# VPC
vpc_cidr = "10.0.0.0/16"

# EC2
instance_type = "t2.nano"
key_name      = "your-key-name"  # TODO: 실제 키 페어 이름으로 변경

# RDS
db_name           = "springvote"
db_username       = "admin"
db_password       = "YourSecurePassword123!"  # TODO: 실제 비밀번호로 변경
db_instance_class = "db.t3.micro"
